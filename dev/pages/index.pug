extends ../pug/layout_default.pug

block pageMain
	section.hero
		.grid
			.hero__inner
				.hero__info
					.hero__row
						.hero__label.hero__label_alt Ремонт от 13 000 ₽ за м² по полу
						.hero__label Работаем только с дизайн-проектом
					h1.hero__title Ремонт квартир и #[span строительство домов] под ключ без стресса в Казани
					ul.hero__list
						li.hero__li Работаем по договору
						li.hero__li Даём гарантию
						li.hero__li Закупаем материалы со скидками
						li.hero__li Понятная, прозрачная смета
						li.hero__li Регулярные фото/видеоотчёты
						li.hero__li Клининг по завершению
				.hero__form
					+form('Получи <span>примерный расчёт</span> и <span>консультацию</span>', 'hero-name', 'hero-phone', 'Получить')
	section.video
		.swiper.video__slider
			.swiper-wrapper
				.swiper-slide
					a.video__frame.video__frame_02(data-hystmodal="#video2")
						svg.video__icon
							use(href='img/svgSprite.svg#icon__play')
				.swiper-slide
					a.video__frame.video__frame_01(data-hystmodal="#video")
						svg.video__icon
							use(href='img/svgSprite.svg#icon__play')
			.swiper-button-prev.video__prev
				svg.swiper-icon
					use(href='img/svgSprite.svg#icon__prev')
			.swiper-button-next.video__next
				svg.swiper-icon
					use(href='img/svgSprite.svg#icon__next')
